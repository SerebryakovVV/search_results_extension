{
    "manifest_version": 3,
    "name": "Links shortcuts",
    "description": "Chrome sucks",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
    {
      "matches": ["https://www.google.com/search*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
  }